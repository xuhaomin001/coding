package xuhaomin;
import java.lang.*;

public class ex1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=3;
		print(3);

	}
	public static void print (int n) {
	    for (int i=0;i<n;i++)
	    {
	    	for(int j=0;j<i;j++) {
	    		System.out.println(i+"*"+j+"="+"j*i");
	    	}
	    }
	}

}
